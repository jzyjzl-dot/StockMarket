{
  "name": "@stock-market/frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts",
    "format": "prettier --write .",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@stock-market/shared": "file:../shared",
    "@stock-market/ui": "file:../ui",
    "axios": "^1.11.0",
    "element-plus": "^2.11.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "vite-plugin-qiankun": "^1.0.15",
    "@eslint/js": "^9.34.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "vite": "^5.1.0",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "vitest": {
    "environment": "jsdom",
    "globals": true
  }
}
